
public class Conta {
	private double saldo;
	private int numero;
	private Cliente cliente;
	private static int contador;
	
	public static int quantidade() {
		return contador++;
	}
	
	public double getSaldo() {
		return saldo;
	}

	public int getNumero() {
		return numero;
	}
	public void setNumero(int numero) {
		this.numero = numero;
	}

}
