import java.util.Scanner;

public class Inputs {

	public static void main(String[] args) {
		
		String x;
		int y;
		
		Scanner variavel = new Scanner(System.in);
		Scanner sc = new Scanner(System.in);
		x = variavel.next();
		y = sc.nextInt();
		System.out.println("Você digitou o valor:" + x + y);
		
		sc.close();
		variavel.close();

	}

}
